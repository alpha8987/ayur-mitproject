<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ChannelDateActivity"
    tools:ignore="ScrollViewCount">

    <RelativeLayout
        android:layout_width="400sp"
        android:layout_height="250sp"
        android:layout_weight=".5">

        <Button
            android:id="@+id/hm_signin4"
            style="@style/Widget.AppCompat.Button.Colored"
            android:layout_width="119dp"
            android:layout_height="39dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="18dp"
            android:layout_marginEnd="15dp"
            android:autofillHints="@string/sign_in_hint"
            android:background="@drawable/button_background"
            android:hint="@string/sign_in_hint"
            android:text="@string/sign_in"
            android:visibility="invisible"/>

        <TextView
            android:id="@+id/dc_docname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="17dp"
            android:layout_marginTop="66dp"
            android:autofillHints="@string/doc_name"
            android:hint="@string/forgot_password_hint"
            android:text="@string/doctor_name"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/dc_docname3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="140dp"
            android:layout_marginTop="69dp"
            android:autofillHints="@string/doc_name"
            android:hint="@string/forgot_password_hint"
            android:text="Sepecailization"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/dc_docname2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="293dp"
            android:layout_marginTop="70dp"
            android:autofillHints="@string/doc_name"
            android:hint="@string/forgot_password_hint"
            android:text="Date"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/cd_docnameview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="21dp"
            android:layout_marginTop="99dp"
            android:autofillHints="@string/doc_name"
            android:hint="@string/doc_name"
            android:text=""
            android:textColor="@color/design_default_color_primary_dark"
            android:textStyle="bold" />


        <TextView
            android:id="@+id/cd_docnameview3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="143dp"
            android:layout_marginTop="99dp"
            android:autofillHints="@string/doc_name"
            android:hint="@string/doc_name"
            android:text=""
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/cd_docnameview2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="295dp"
            android:layout_marginTop="97dp"
            android:autofillHints="@string/doc_name"
            android:hint="@string/doc_name"
            android:text="Date"
            android:textColor="#000000"
            android:textStyle="bold" />


        <View
            android:id="@+id/divider3"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="140dp"
            android:background="@android:color/holo_blue_dark" />


        <ListView
            android:id="@+id/doctor_appointment_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/divider3"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="45dp"
            android:layout_marginBottom="-1dp"
            android:layout_weight="1.86" />

    </RelativeLayout>


</ScrollView>